{
 "name": "nodejs-test-app",
 "version": "1.0.0",
 "description": "NodeJS Test App",
 "main": "app.js",
 "scripts": {
  "start": "babel-watch app.js",
  "test": "ava --watch"
 },
 "author": "",
 "license": "ISC",
 "dependencies": {
  "babel-cli": "^6.26.0",
  "babel-preset-node6": "^11.0.0",
  "bcrypt": "^1.0.3",
  "body-parser": "^1.14.0",
  "dotenv": "^4.0.0",
  "express": "^4.13.3",
  "jsonschema": "^1.2.0",
  "jsonwebtoken": "^7.3.0",
  "mysql": "^2.9.0",
  "mysql2": "^1.5.0",
  "sequelize": "^4.22.6",
  "sequelize-cli": "^3.0.0"
 },
 "devDependencies": {
  "ava": "^0.23.0",
  "babel-preset-es2015": "^6.24.1",
  "babel-watch": "^2.0.7"
 },
 "ava": {
  "files": [
   "test/*.js"
  ],
  "source": [
   "**/*.{js,jsx}"
  ],
  "concurrency": 5,
  "failFast": true,
  "failWithoutAssertions": false,
  "tap": true,
  "powerAssert": false,
  "require": [
   "babel-register"
  ],
  "babel": "inherit"
 }
}
